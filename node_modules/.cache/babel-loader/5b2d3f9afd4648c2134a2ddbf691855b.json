C,YAAT,CAAsB,KAAK,UAAL,GAAgB,GAAhB,GAAoB,IAA1C,CAA3B;AACA,IAAA,kBAAkB,CAAC,KAAnB,GAA2B,SAA3B;AACA,IAAA,kBAAkB,CAAC,YAAnB,CAAgC,WAAhC,GAA8C,KAAK,UAAL,GAAgB,GAAhB,GAAoB,IAAlE;AACA,IAAA,kBAAkB,CAAC,OAAnB,GAA6B,CACzB;AAAC,MAAA,MAAM,EAAE,MAAT;AAAiB,MAAA,GAAG,EAAE,eAAtB;AAAuC,MAAA,KAAK,EAAE;AAA9C,KADyB,EAEzB;AAAC,MAAA,MAAM,EAAE,MAAT;AAAiB,MAAA,GAAG,EAAE,eAAtB;AAAuC,MAAA,KAAK,EAAE;AAA9C,KAFyB,EAGzB;AAAC,MAAA,MAAM,EAAE,WAAT;AAAsB,MAAA,GAAG,EAAE,sBAA3B;AAAmD,MAAA,KAAK,EAAE;AAA1D,KAHyB,EAIzB;AAAC,MAAA,MAAM,EAAE,SAAT;AAAoB,MAAA,GAAG,EAAE,oBAAzB;AAA+C,MAAA,KAAK,EAAE;AAAtD,KAJyB,CAA7B;AAMA,SAAK,aAAL,CAAmB,OAAnB,CAA2B,UAAA,QAAO,EAAK;AACnC,MAAA,kBAAkB,CAAC,MAAnB,CACI,GAAG,CAAC,MAAJ,GAAa;AACb,QAAA,IAAI,EAAE,QAAQ,CAAC,IADF;AAEb,QAAA,IAAI,EAAE,QAAQ,CAAC,IAFF;AAGb,QAAA,oBAAoB,EAAE,QAAQ,CAAC,UAHlB;AAIb,QAAA,kBAAkB,EAAE,QAAQ,CAAC;AAJhB,OADjB;AAOH,KARD;AASA,IAAA,OAAO,CAAC,GAAR,CAAY,QAAZ;AACH;AA1EU,CAAf","sourcesContent":["<template>\r\n\r\n    <div class=\"list\">\r\n        <div>\r\n            <p id=\"title\"> Attendees </p>\r\n            <button id=\"downloadListButton\" class=\"toolButton\" v-on:click=\"downloadAttendeesList\"><i class=\"fas fa-file-download\"></i></button>\r\n        </div>\r\n        <ul id=\"attendeesList\">\r\n            <li v-bind:id=\"'attendee'+attendee.id\" v-for=\"attendee in attendeesList\" v-bind:key=\"attendee.id\" class=\"listItem\">\r\n                <div class=\"dropdown\">\r\n                    <button id=\"attendeeOptions\" style=\"background: white ; border:none\" v-on:click=\"showDropdown(this)\"><i class=\"fas fa-bars\"></i></button>\r\n                    <div class=\"dropdown-content-shapes\" id=\"dropdownRoles\">\r\n                        <button id=\"listenerRole\" v-on:click=\"setRole(attendee.id, 'listener', $event)\">Listener</button>\r\n                        <button id=\"speakerRole\" v-on:click=\"setRole(attendee.id, 'speaker')\">Speaker</button>\r\n                        <button id=\"presenterRole\" v-on:click=\"setRole(attendee.id, 'presenter')\">Presenter</button>\r\n                        <button id=\"hostRole\" v-on:click=\"setRole(attendee.id, 'host')\">Host</button>\r\n                    </div>\r\n                </div>\r\n                {{ attendee.name }}\r\n                <i class=\"fas fa-assistive-listening-systems\" id=\"listenerIcon\" v-show=\"attendee.role == 'listener'\" style=\"float:right\"></i>\r\n                <i class=\"fas fa-microphone\" id=\"speakerIcon\" v-show=\"attendee.role == 'speaker'\"  style=\"float:right;\"></i>\r\n                <i class=\"fas fa-user-alt\" id=\"presenterIcon\" v-show=\"attendee.role == 'presenter'\" style=\"float:right;\"></i>\r\n                <i class=\"fas fa-user-tie\" id=\"hostIcon\" v-show=\"attendee.role == 'host'\" style=\"float:right;\"></i>\r\n            </li>\r\n        </ul>\r\n    </div>\r\n\r\n</template>\r\n\r\n\r\n<script>\r\nimport '@fortawesome/fontawesome-free/js/all'\r\nrequire('core-js/modules/es.promise')\r\nrequire('core-js/modules/es.string.includes')\r\nrequire('core-js/modules/es.object.assign')\r\nrequire('core-js/modules/es.object.keys')\r\nrequire('core-js/modules/es.symbol')\r\nrequire('core-js/modules/es.symbol.async-iterator')\r\nrequire('regenerator-runtime/runtime')\r\nconst ExcelJS = require('exceljs/dist/es5')\r\n\r\nwindow.onclick = function(e){\r\n    if(e.target.parentElement.id !== \"attendeeOptions\" && e.target.id !== \"attendeeOptions\"){\r\n        document.getElementsByClassName(\"dropdown\").forEach(dp => {\r\n            dp.childNodes.forEach( e => {\r\n                if(e.id === \"dropdownRoles\"){\r\n                    e.style.display = \"none\"\r\n                }\r\n            } )\r\n        })\r\n    }\r\n}\r\n\r\nexport default {\r\n    name: \"Atendees\",\r\n    data(){\r\n        return {\r\n            attendeesList : [\r\n                {name: \"Alireza Davoodi\", id: \"1\", role: \"listener\", joinedTime: \"\", leftTime: \"\"},\r\n                {name: \"Adrian Jefferson\", id: \"2\", role: \"host\", joinedTime: \"\", leftTime: \"\"},\r\n                {name: \"Micheal Jackson\", id:\"3\", role: \"speaker\", joinedTime: \"\", leftTime: \"\"},\r\n                {name: \"Mason Mount\", id:\"4\", role: \"presenter\", joinedTime: \"\", leftTime: \"\"}\r\n            ],\r\n        } \r\n    },\r\n    props: [\r\n        'host',\r\n        'classtitle'\r\n    ],\r\n    methods: {\r\n        setRole(id, role){\r\n            document.getElementsByClassName(\"listItem\").forEach(el => {\r\n                if(el.id === `attendee${id}`){\r\n                    el.childNodes.forEach(child => {\r\n                        if(child.id !== undefined){\r\n                            if(child.id.includes(\"Icon\")){\r\n                                child.style.display = \"none\"\r\n                            }\r\n                            if(child.id === `${role}Icon`){\r\n                                child.style.display = \"inline-block\"\r\n                            }\r\n                        }\r\n                    })\r\n                }\r\n            })\r\n            this.attendeesList.forEach(el => {\r\n                if(el.id === id){\r\n                    el.role = role\r\n                }\r\n                \r\n            })\r\n        },\r\n        showDropdown(e){\r\n            var dropdownRoot = event.target.parentElement.parentElement\r\n            dropdownRoot.childNodes.forEach(e => {\r\n                if(e.id === \"dropdownRoles\"){\r\n                    e.style.display = \"block\"\r\n                }\r\n            })\r\n        }\r\n    },\r\n    downloadAttendeesList(){\r\n        var workbook = new ExcelJS.Workbook()\r\n        workbook.creator = this.host\r\n        workbook.lastModifiedBy = this.host\r\n        var date = new Date()\r\n        date = new Date.parse(date.getFullYear, date.getMonth, date.getDay, date.getHours, date.getMinutes, date.getSeconds)\r\n        workbook.created = date\r\n        const attendessWorksheet = workbook.addWorksheet(this.classtitle+\" \"+date)\r\n        attendessWorksheet.state = 'visible'\r\n        attendessWorksheet.headerFooter.firstHeader = this.classtitle+\" \"+date\r\n        attendessWorksheet.columns = [\r\n            {header: \"name\", key: \"attendee_name\", width: 10},\r\n            {header: \"role\", key: \"attendee_role\", width: 10},\r\n            {header: \"joined at\", key: \"attendee_joined_time\", width: 10},\r\n            {header: \"left at\", key: \"attendee_left_time\", width: 10}\r\n        ]\r\n        this.attendeesList.forEach(attendee => {\r\n            attendessWorksheet.addRow(\r\n                row.values = {\r\n                name: attendee.name,\r\n                role: attendee.role,\r\n                attendee_joined_time: attendee.joinedTime,\r\n                attendee_left_time: attendee.leftTime\r\n            })\r\n        })\r\n        console.log(workbook);\r\n    },\r\n\r\n}\r\n\r\n</script>\r\n\r\n\r\n<style scoped>\r\n    .list {\r\n        width: 22%;\r\n        height: 50% auto;\r\n        background-color: rgb(38, 3, 95);\r\n        float: right;\r\n        border: 2px solid;\r\n        border-radius: 5px;\r\n        margin: 5px;\r\n    }\r\n    #attendeesList {\r\n        border: 2px solid white;\r\n        border-radius: 5px;\r\n        align-items: center;\r\n        padding: 2px;\r\n        margin: 5px;\r\n    }\r\n    #attendee {\r\n        padding: 3px;\r\n        background: white;\r\n        list-style-type: none;\r\n        border: 2px solid black;\r\n        font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;\r\n    }\r\n    .listItem {\r\n        padding: 3px;\r\n        background: white;\r\n        list-style-type: none;\r\n        border: 2px solid black;\r\n        font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;\r\n    }\r\n    .listItem:hover {\r\n        background: #43e5f7;\r\n    }\r\n    #title {\r\n        text-align: center;\r\n        color: rgb(255, 255, 255);\r\n        font-weight: bold;\r\n        align-content: center;\r\n        display: inline-block;\r\n        padding: 2px;\r\n        margin: 2px;\r\n        font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;\r\n    }\r\n    #active {\r\n        display: inline;\r\n    }\r\n    .dropdown-content-shapes {\r\n        display: none;\r\n        position: absolute;\r\n        background-color: #ffffff;\r\n        align-content: center;\r\n        box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);\r\n        z-index: 1;\r\n    }\r\n    .dropdown {\r\n        position: relative;\r\n        display: inline-block;\r\n    }\r\n    .dropdown-content-shapes button {\r\n        color: black;\r\n        padding: 5px;\r\n        width: 100%;\r\n        display: block;\r\n        outline: none;\r\n        border: none;\r\n        background: white;\r\n    }\r\n    .dropdown-content-shapes button:hover {background-color:#43e5f7;}\r\n\r\n    .toolButton {\r\n        font-family: Arial, Helvetica, sans-serif;\r\n        font-weight: bold;\r\n        background: white;\r\n        padding: 5px;\r\n        border-radius: 5px;\r\n        border: 2px solid rgb(41, 40, 40);\r\n        align-content: center;\r\n        color: black;\r\n        width: 30px;\r\n        height: 30px;\r\n    }\r\n    .toolButton:hover {\r\n        background: #43e5f7;\r\n    }\r\n    #downloadListButton {\r\n        float: right;\r\n        margin-bottom: 3px;\r\n    }\r\n    \r\n</style>"],"sourceRoot":""}]}