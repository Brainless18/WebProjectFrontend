n\t\t\t\t})\r\n\t\t\t\tvar uId \r\n\t\t\t\tawait this.$props.userId.then(value => {\r\n\t\t\t\t\tuId = value\r\n\t\t\t\t})\r\n\t\t\t\tconsole.log(mId, uId);\r\n\t\t\t\tawait axios.post(this.api_messages_url, {\r\n\t\t\t\t\t\r\n\t\t\t\t\t\"sender\": uId, \"session\": mId, \"content\": this.chatBody\r\n\t\t\t\t\t}).then(response => {\r\n\t\t\t\t\t\tconsole.log(response);\r\n\t\t\t\t\t}).catch(error => {\r\n\t\t\t\t\t\tconsole.error(error);\r\n\t\t\t\t\t})\r\n\t\t\t}\r\n\t\t},\r\n\t\tasync initStream(){\r\n\t\t\tthis.client = new StreamChat()\r\n\t\t}\r\n\t},\r\n\tmounted(){\r\n\t\t\r\n\t},\r\n\tprops:[\r\n\t\t'userId',\r\n\t\t'meetingId'\r\n\t],\r\n\t\r\n\t\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\n\t.chatMessages{\r\n\t\theight: 70%;\r\n\t}\r\n\t#sendButton{\r\n\t\tbackground: white;\r\n\t\tcolor: black;\r\n        border-radius: 5px;\r\n        border: 2px solid rgb(41, 40, 40);\r\n        align-content: center;\r\n        margin: 5px;\r\n        width: 30px;\r\n        height: 30px;\r\n\t\tfloat: inline-end;\r\n\t}\r\n\t#sendButton:hover{\r\n\t\tbackground: #43e5f7;\r\n\t}\r\n\t.sendMessageDiv{\r\n\t\tmargin: 5px;\r\n\t\tmargin-top: 20px;\r\n\t\twidth: 100%;\r\n\t\theight: 30%;\r\n\t}\r\n\t#sendMessageContent{\r\n\t\tborder: 2px solid black;\r\n\t\tborder-radius: 5px;\r\n\t\tbackground: white;\r\n\t\tpadding: 5px;\r\n\t\twidth: 78%;\r\n\t}\r\n\t#messagesList{\r\n\t\toverflow-y: scroll;\r\n\t\tbackground: white;\r\n\t\tmargin: 5px;\r\n\t\tborder: 2px solid black;\r\n\t\tborder-radius: 5px;\r\n\t\tlist-style: none;\r\n\t\tpadding: 5px;\r\n\t\theight: 100%;\r\n\t}\r\n\t.message {\r\n\t\tfont-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;\r\n\t\tfont-size: 10px;\r\n\t\tfont-weight: bold;\r\n\t}\r\n\t::-webkit-scrollbar {\r\n\t\twidth: 10px;\r\n\t\tborder: 2px solid black;\r\n\t\tborder-radius: 5px;\r\n\t}\r\n\r\n\t/* Track */\r\n\t::-webkit-scrollbar-track {\r\n\t\tbackground: #f1f1f1; \r\n\t\tborder: 2px;\r\n\t\tborder-radius: 2px;\r\n\