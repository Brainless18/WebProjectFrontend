tIsDraw\" v-on:mousemove=\"draw\" v-on:mousedown=\"setIsDraw\" v-on:mouseover=\"changeCursor\" resize>Sorry, your browser does not support HTML5 canvas technology.</canvas>    \r\n    </div>\r\n    \r\n</template>\r\n\r\n\r\n<script>\r\n\r\nvar canvas, ctx, canvasX, canvasY;\r\nwindow.onload = function(){\r\n    canvas = document.getElementById(\"whiteboard_canvas\");\r\n    ctx = canvas.getContext(\"2d\")\r\n    ctx.fillStyle = '#fff'\r\n    ctx.fillRect(0, 0, canvas.offsetWidth, canvas.offsetHeight);\r\n    ctx.lineWidth = 1;\r\n    ctx.lineHeight = 1;\r\n    \r\n}\r\n\r\nexport default {\r\n    data() {\r\n        \r\n        return {\r\n            instanceNumber: 0,\r\n            pencilColor: 'black',\r\n            eraseColor: '#fff',\r\n            drawColor: '#000',\r\n            mode: 'draw',\r\n            isDrawing: false,\r\n            pencilThickness: 0.5\r\n        }\r\n    },\r\n    methods: {\r\n        setIsDraw(e){\r\n            if(e.type === 'mouseup'){\r\n                this.isDrawing = false\r\n            }else {\r\n                if(typeof ctx !== 'undefined'){\r\n                    this.isDrawing = true\r\n                    ctx.beginPath();\r\n                    canvasX = e.pageX - canvas.offsetLeft;\r\n                    canvasY = e.pageY - canvas.offsetTop;\r\n                    ctx.moveTo(canvasX, canvasY);   \r\n                }\r\n            }\r\n        },\r\n        draw(e){\r\n            if(this.mode == 'erase'){\r\n                this.pencilColor = this.eraseColor\r\n            }\r\n            else if(this.mode == 'draw'){\r\n                this.pencilColor = this.drawColor\r\n            }\r\n            if(typeof ctx !== 'undefined'){\r\n                if(this.isDrawing !== false){\r\n                    canvasX = e.pageX - canvas.offsetLeft;\r\n                    canvasY = e.pageY - canvas.offsetTop;\r\n                    ctx.lineTo(canvasX, canvasY);\r\n                    ctx.strokeStyle = this.pencilColor;\r\n                    ctx.stroke();\r\n                }\r\n                else if(this.isDrawing === false){\r\n                    ctx.closePath()\r\n                }\r\n            }\r\n        },\r\n        changeColor(color){\r\n            this.pencilColor = color;\r\n        },\r\n        setMode(mode){\r\n            this.mode = mode\r\n            if (mode == 'erase'){\r\n                document.getElementById(mode).disabled = true\r\n                document.getElementById('draw').disabled = false\r\n            }\r\n            if (mode == 'draw'){\r\n                document.getElementById(mode).disabled = true\r\n                document.getElementById('erase').disabled = false\r\n            }\r\n            \r\n        },\r\n        changeCursor(){\r\n            if( typeof canvas !== 'undefined'){\r\n                canvas.style.cursor = 'crosshair'\r\n            }\r\n        }\r\n        \r\n    }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n    .whiteboard {\r\n        width: 100%;\r\n        height: 100%;\r\n        background-color: rgb(151, 151, 151);\r\n        border-radius: 5px;\r\n        border-color: black;\r\n        text-align: start;\r\n        padding: 5px;\r\n    }\r\n    #whiteboard_client[resize] {\r\n       position: relative;\r\n       resize: both;\r\n    }\r\n    p.title {\r\n        color: black;\r\n        font-weight: bold;\r\n    }\r\n    #tools {\r\n        margin: 5px;\r\n        padding: 5px;\r\n        justify-content: space-between;\r\n    }\r\n    #colorPicker {\r\n        margin-left: 5px;\r\n        border: 2px;\r\n    }\r\n</style>\r\n"],"sourceRoot":""}]}